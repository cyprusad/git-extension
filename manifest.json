{
  "manifest_version": 2,
  "name": "GitHub.com Terminal",
  "description": "This extension helps you use native shell commands in the
                  browser from within the browser",
  "version": "1.0",
  "icons": {},
  "permissions": [
    "tabs",
    "bookmarks",
    "history",
    "clipboardRead",
    "storage",
    "<all_urls>"
  ],
  "page_action": {
      "default_icon": "icons/icon.png",
      "default_title": "GitHub.com Terminal"
    },
  "background": {
    "scripts": ["content_scripts/background.js"],
    "persistent": false
  }
}
